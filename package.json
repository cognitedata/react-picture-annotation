{
  "name": "@cognite/react-picture-annotation",
  "version": "0.2.4",
  "description": "A simple annotation component.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "dist/types/src/index.d.ts",
  "scripts": {
    "build": "tsc --build && rollup -c ./rollup.config.ts",
    "storybook": "start-storybook -p 6006 --debug-webpack --no-dll",
    "build-storybook": "build-storybook",
    "commit": "npx git-cz",
    "precommit": "lint-staged",
    "lib:publish": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kunduin/react-picture-annotation.git"
  },
  "keywords": [
    "react",
    "typescript-library",
    "javascript-library",
    "annotation"
  ],
  "author": "Bay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kunduin/react-picture-annotation/issues"
  },
  "homepage": "https://github.com/Kunduin/react-picture-annotation#readme",
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@svgr/rollup": "^5.4.0",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/storybook__react": "^5.2.1",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.6",
    "prettier": "^2.0.5",
    "react-docgen-typescript-loader": "^3.7.2",
    "rollup": "^2.11.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-url": "^3.0.1",
    "stylelint": "^13.5.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^26.0.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@types/pdfjs-dist": "^2.1.4",
    "pdfjs-dist": "^2.3.200",
    "prop-types": "^15.7.2",
    "reselect": "^4.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "warning": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "{stories,src}/**/*.css": "stylelint",
    "{stories,src}/**/*.{ts,tsx}": [
      "tslint --project tsconfig.json -c tslint.json --fix",
      "prettier --write",
      "git add"
    ]
  }
}